/* pages/mine/myPaper.wxss */
.background{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
}
.background image{
  width: 100%;
  height: 100%;
}
.fartherBox{
  width: 95%;
  padding-top: 120rpx;
  margin-left: auto;
  margin-right: auto;
}
.time{
  height: 50rpx;
  padding-left: 36rpx;
  padding-right: 36rpx;
  padding-bottom: 10rpx;
  padding-top: 10rpx;
  font-size: 34rpx;
  font-weight: 400;
  color: #707070;
  /* background-color: #bfbfbf90; */
}
.custom-image .van-empty__image {
  width: 90px;
  height: 90px;
}
.van-cell{
  height: 120rpx !important;
  background-color: #ffffff8a !important;
  border: solid 2rpx #dadada67;
  border-radius: 20rpx !important;
}
.van-cell__title{
  padding-top: 15rpx !important;
  font-size: 36rpx !important;
  font-weight: 500;
}
.van-cell__value{
  padding-top: 15rpx !important;
}
.detele{
  width:130rpx;
  height:88rpx;
  background-color:rgba(0, 199, 40, 0.899); 
  color:#ffffff;
  text-align:center;
  padding-top:35rpx;
  border-radius: 20rpx 0 0 20rpx;
}
.detail{
  margin-top: 20rpx;
  width: 90%;
  height: 90%;
  margin-left: auto;
  margin-right: auto;
}
.text{
  width: 400rpx;
  margin-left: auto;
  margin-right: auto;
  text-align: center;
  font-size: 36rpx;
  padding: 20rpx;
  border-bottom: solid 2rpx #dadada67;
}
