.bg{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
  top: 0;
}
.farther-box{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.logo{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-top: 100rpx;
}
.from{
  width:85%;
  margin-top: 100rpx;
  border-radius: 20rpx;
}
.van-cell{
  margin-top: 20rpx;
  background-color: rgb(177, 177, 177, 0.6) !important;
  font-size: 34rpx !important;
  border-radius: 20rpx;
}
.field-index--van-field__label{
  border-right: solid 1rpx #ffffff !important;
  color: #ffffff !important;
}
.van-field__placeholder{
  font-size: 30rpx;
  color: rgba(255,255,255,0.8) !important;
}
.tap{
  font-weight: 700;
  font-size: 28rpx;
  width: 300rpx;
  margin-left: 430rpx;
  margin-top: -82rpx;
  border: 0;
  z-index: 2;
}
button::after{
  border:none;
}
.registerButton{
  width: 300rpx;
  height: 90rpx;
  color: #ffffff;
  background-color: #4cd1b0;
  box-sizing: border-box;
  border-radius: 20rpx;
  position: fixed;
  bottom: 200rpx;
  text-align: center;
  font-size: 38rpx;
  letter-spacing: 5px;
  font-weight: 600;
}
.tips{
  display: flex;
  flex-direction: row;
  bottom: 20rpx;
  position: fixed;
}
.contact{
  color: #4cd1b0;
}
.mask{
  margin-left: 80rpx;
  z-index: 9999;
  height: 80%;
  width: 600rpx;
  background-color: #ffffff;
  border-radius: 5rpx;
  position: fixed;
  top: 200rpx;
  border: solid 2rpx #ccc;
}
.mask .title{
  z-index: 9999;
  margin-top: 40rpx;
  font-size: 45rpx;
  text-align: center;
  font-weight: 750;
}

.mask .content{
  z-index: 9999;
  margin-top: 20rpx;
  width: 86%;
  margin-left: 7%;
  align-items: center;
  height: 82%;
}
.mask .content .f{
  z-index: 9999;
  font-size: 36rpx;
  font-weight: 500;
  letter-spacing: 5rpx;
  padding: 10rpx;
  color: #4cd1b0;
}
.mask .content .s{
  z-index: 9999;
  font-size: 32rpx;
  font-weight: 200;
  letter-spacing: 5rpx;
}