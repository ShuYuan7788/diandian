.type-container{
  height: 100rpx;
  display: flex;
  align-items: center;
}

.type-text{
  font-size: 45rpx;
  font-weight: 800;
  margin-left: 40rpx;
}

.choice{
  height: 120rpx;
  width: 84%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin:8%;
  border-radius: 20rpx;
  box-shadow: 0px 3px 10px 2px #eeeeee;
  background-color: white;
}

.choice-box{
  width: 200rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.more-img{
  width: 60rpx;
  height: 60rpx;
}

.cell-item-container{
  margin-top: 5%;
  border-radius: 15rpx;
  box-shadow: 0px 0px 5px 3px #f2f2f2;
}

.cell-container{
  width: 94%;
  margin-left: 3%;
  margin-right: 3%;
}