.collect-icon {
  width: 50rpx;
}

.title-container{
  position: fixed;
  top: 0;
  z-index: 90;
  width: 100%;
  height: 78rpx;
  border-bottom: 1px solid #e4e4e4;
  background-color: white;
}

.title-group{
  padding: 0 50rpx;
  height: inherit;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.counter{
  font-size: 30rpx;
  color: #999999;
  margin-right: 288rpx;
}

.question-context-container {
  margin-top: 85rpx;
  width: 700rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
  margin-left: auto;
  margin-right: auto;
}

.question-context {
  font-size: 32rpx !important;
  line-height: 55rpx !important;
  color: rgb(46, 46, 46) !important;
}

.question-img-container {
  margin-top: 30rpx;
}

.question-img {
  width: 640rpx;
}

.bottom {
  height: 300rpx;
  width: 100%;
}

.radio-container {
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
}

.radio-item {
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
}

.radio-container .radio-item-after {
  transition:background-color 0.5s ease;
  background-color: #A6ECFF !important;
  color: white;
}

.checkbox-container {
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
}

.checkbox-item {
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
}

.checkbox-container .checkbox-item-after {
  transition:background-color 0.5s ease;
  background-color: #A6ECFF !important;
  color: white;
}

/* 消除cellvalue样式 */
.value-class {
  flex: none !important;
}

.judge-container {
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
}

.judge-item {
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
}

.judge-container .judge-item-after {
  transition:background-color 0.5s ease;
  background-color: #A6ECFF !important;
  color: white;
}

.input-container {
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
}

.Comprehension-container {
  width: 660rpx;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 5px 0px #e7e7e7;
  padding: 20rpx;
}

.Comprehension-context-container {
  margin-left: auto;
  margin-right: auto;
  min-height: 600rpx;
}

.Comprehension-img-container {
  padding-left: 30rpx;
}

.Comprehension-context-input {
  background-color: #f7f7f769 !important;
  min-height: 600rpx !important;
  padding: 20rpx !important;
}